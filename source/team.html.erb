      <div class="container text-center padded">
        <h1>Our team</h1>
        <ul class="list-inline">
          <% %w(elephant lion monkey).each do |member| %>
          <li>
            <h3><%= member %></h3>
            <%= image_tag "#{member}.jpg", class: "avatar-large" %>
          </li>
          <%end %>
        </ul>
      </div>



        <h3 class="padded text-center">

            <% link_to 'mailto:contact@airbnb.com' do %>
                  <i class="fa fa-envelope"></i>
                  contact@airbnb.com
            <% end %>
        </h3>
      </div>
